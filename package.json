{
  "name": "automart-3",
  "version": "2.0.0",
  "description": "Auto Mart is an online marketplace for automobiles of diverse makes, model or body type. With Auto Mart, users can sell their cars or buy from trusted dealerships or private sellers.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm run createTables && npm run admin && NODE_ENV=development nodemon --exec babel-node ./server/server.js",
    "start": "npm run createTables && npm run admin && node --require @babel/register ./server/server.js",
    "test": "NODE_ENV=test npm run droptablesTests && NODE_ENV=test npm run createTablesTests && NODE_ENV=test npm run admin && NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --timeout 5000  --require @babel/register test/*js --exit || true",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "createTablesTests": "NODE_ENV=test node --require @babel/register ./server/models/tables.js",
    "droptablesTests": "NODE_ENV=test node --require @babel/register ./server/models/dropTables.js",
    "createTables": "node  --require @babel/register ./server/models/tables.js",
    "dropTables": "node --require @babel/register ./server/models/dropTables.js",
    "admin": "node --require @babel/register ./server/models/admin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raymond42/Auto-Mart.git"
  },
  "author": "Raymond Gakwaya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raymond42/Auto-Mart/issues"
  },
  "homepage": "https://github.com/raymond42/Auto-Mart#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "ci": "^1.0.0",
    "cli": "^1.0.1",
    "coveralls": "^3.0.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "heroku": "^7.25.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "swagger-node-express": "^2.1.3",
    "swagger-ui-express": "^4.0.4"
  }
}
